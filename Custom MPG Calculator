#include <iostream>

using namespace std;

int main()
{
    double tgallons = 15;
   cout << "Please enter the cars total gallons" << endl;
    cin >> tgallons;
    double tmiles = 375;
    cout << "Please enter the cars total miles" << endl;
    cin >> tmiles;
    double mpg = tmiles/ tgallons;


    cout << "The cars MPG is " << mpg << endl;

    return 0;
}
